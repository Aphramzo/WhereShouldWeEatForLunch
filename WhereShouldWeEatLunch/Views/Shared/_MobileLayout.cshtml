<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    
    <meta name="viewport" content="width=device-width, initial-scale=1"> 
	<link rel="stylesheet" href="http://code.jquery.com/mobile/1.2.0/jquery.mobile-1.2.0.min.css" />
</head>
    <body>
        <nav data-role="navbar">
        <ul>
            <li>
                @Html.ActionLink("Home", "Index","Search")
            </li>
            <li>
                @Html.ActionLink("Eateries", "Index","Eatery")
            </li>
            <li>
                @Html.ActionLink("Styles", "Index","FoodStyle")
            </li>
        </ul>
    </nav>
        @RenderBody()
    </body>
</html>
@MvcHtmlString.Create(
        Bundle.JavaScript()
            .Add("/Scripts/jquery-1.8.2.min.js")
            .Add("/Scripts/jquery.mobile-1.2.0.min.js")
            .Add("/Scripts/modernizr-1.7.min.js")
        .Add("/Scripts/modernizr-1.7.min.js")
        .Render("/Scripts/combine-mobile.js")
    )
    @RenderSection("js", false)
