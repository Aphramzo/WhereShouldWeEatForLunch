@model WhereShouldWeEatLunch.Models.FourSquareViewModel

@{
    ViewBag.Title = "title";
}

@section js
{
    <!--Cannot figure out why, but geo-min here doesnt like being bundled -->
    <script src="@Url.Content("~/Scripts/geo-min.js")" type="text/javascript"></script>   
    @MvcHtmlString.Create(
        Bundle.JavaScript()
        .Add("/Scripts/foursquare.js")
        .Render("/Scripts/FourSquareList.js")
    )
}
<h2>Powered By FourSquare</h2>
<div class="editor-label">What kind of food are you in the mood for?</div>
<div class="editor-field">@Html.DropDownList("foodStyle",Model.Categories, new {Multiple = "multiple"})</div>
<div id="eateryList" data-role="collapsible-set" >
        Awaiting Location...
</div>

<div data-role="collapsible-set">
<div id="collapsableTemplate" class="hidden">
    <div class="collapsible" >
        <h3>{0}</h3>
        <p>
            <span class="left"><a href="http://maps.google.com/?daddr={2},{3}">Directions ({1} mi.)</a></span>
            <span class="right">{4}</span>
            <br /><br />
            <span class="left">{5}</span>
            <span class="right"><a data-ajax="false" href="/FourSquare/Details/{6}">See more details</a></span>
        </p>
    </div>
</div>
</div>